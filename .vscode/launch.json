{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Executar index.ts",
      "program": "${workspaceFolder}/index.ts",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["tsx", "index.ts"],
      "cwd": "${workspaceFolder}",
      "env": {
        "SEND_DELAY_MS": "2000"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
